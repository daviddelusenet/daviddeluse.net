{
  "name": "daviddeluse.net",
  "version": "1.0.0",
  "description": "Website of David de Lusenet",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build:dev",
    "build:dev": "BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
    "build:prod": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
    "dist": "npm run build:prod",
    "dist:analyze": "BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production --env.analyze=true",
    "lint:css": "stylelint './src/**/*.js' --config config/.stylelintrc.js",
    "lint:js": "eslint './src/**/*.js' --config config/.eslintrc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviddelusenet/daviddeluse.net.git"
  },
  "keywords": [
    "David de Lusenet",
    "portfolio",
    "website",
    "React",
    "Styled Components"
  ],
  "author": "David de Lusenet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviddelusenet/daviddeluse.net/issues"
  },
  "homepage": "http://daviddeluse.net",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "gsap": "^2.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.1",
    "styled-reset": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "stylelint": "^9.8.0",
    "stylelint-config-concentric-order": "^3.4.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
