{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "noUnusedLocals": true,
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
    },
    "exclude": [
        "node_modules",
    ],
    "include": [
        "src/**/*",
    ],
}
